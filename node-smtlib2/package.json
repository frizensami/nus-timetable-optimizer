    "@types/byline": "^4.2.32",
    "byline": "^5.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.19",
